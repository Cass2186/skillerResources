<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="263.0" prefWidth="365.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scripts.tabMaker.Data.gui.TabGUIController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
            <MenuItem mnemonicParsing="false" text="Open…" />
            <Menu mnemonicParsing="false" text="Open Recent" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Close" />
            <MenuItem mnemonicParsing="false" text="Save" />
            <MenuItem mnemonicParsing="false" text="Save As…" />
            <MenuItem mnemonicParsing="false" text="Revert" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="213.0" prefWidth="390.0" VBox.vgrow="ALWAYS">
         <children>
            <ComboBox fx:id="selectTabComboBox" layoutX="106.0" layoutY="86.0" prefHeight="25.0" prefWidth="156.0" promptText="Select Tab" />
            <Button fx:id="startScriptButton" layoutX="141.0" layoutY="185.0" mnemonicParsing="false" onAction="#startButtonPressed" text="Start Script">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Label layoutX="129.0" layoutY="39.0" text="cTab Maker">
               <font>
                  <Font name="Arial Bold" size="20.0" />
               </font>
            </Label>
            <CheckBox fx:id="makeProfitableTabButton" layoutX="106.0" layoutY="135.0" mnemonicParsing="false" text="Make most profitable tab" />
         </children>
    </AnchorPane>
  </children>
</VBox>
