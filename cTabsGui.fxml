<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="263.0" prefWidth="365.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scripts.tabMaker.Data.gui.TabGUIController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
            <MenuItem mnemonicParsing="false" text="Open…" />
            <Menu mnemonicParsing="false" text="Open Recent" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Close" />
            <MenuItem mnemonicParsing="false" text="Save" />
            <MenuItem mnemonicParsing="false" text="Save As…" />
            <MenuItem mnemonicParsing="false" text="Revert" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="213.0" prefWidth="390.0" VBox.vgrow="ALWAYS">
         <children>
            <ComboBox fx:id="selectTabComboBox" layoutX="106.0" layoutY="68.0" prefHeight="25.0" prefWidth="156.0" promptText="Select Tab" />
            <Button fx:id="startScriptButton" layoutX="140.0" layoutY="195.0" mnemonicParsing="false" onAction="#startButtonPressed" text="Start Script">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Label layoutX="130.0" layoutY="26.0" text="cTab Maker">
               <font>
                  <Font name="Arial Bold" size="20.0" />
               </font>
            </Label>
            <CheckBox fx:id="makeProfitableTabButton" layoutX="104.0" layoutY="100.0" mnemonicParsing="false" text="Make most profitable tab" />
            <Slider fx:id="abc2Percent" layoutX="151.0" layoutY="138.0" min="15.0" prefHeight="38.0" prefWidth="138.0" showTickLabels="true" showTickMarks="true" snapToTicks="true" value="50.0" />
            <Label layoutX="60.0" layoutY="138.0" text="\% ABC2 Sleep:" />
         </children>
    </AnchorPane>
  </children>
</VBox>
